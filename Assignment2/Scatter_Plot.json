{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 400,
    "title": "Life Expectancy vs. Birth Rate by Continent",
    "data": {
      "url": "https://raw.githubusercontent.com/4ryanShah/FIT3179/main/Assignment2/world-data-2023-aryan.csv",
      "format": {"type": "csv"}
    },
    "mark": "circle",
    "encoding": {
      "x": {
        "field": "Life expectancy",
        "type": "quantitative",
        "title": "Life Expectancy",
        "scale": {"domain": [4, 48]}
      },
      "y": {
        "field": "Birth Rate",
        "type": "quantitative",
        "title": "Birth Rate",
        "scale": {"domain": [53, 84]}
      },
      "size": {
        "field": "Population",
        "type": "quantitative",
        "scale": {"range": [10, 300], "zero": false},
        "title": "Population"
      },
      "color": {
        "field": "Country",
        "type": "nominal",
        "legend": null
      },
      "tooltip": [
        {"field": "Country", "type": "nominal", "title": "Country"},
        {"field": "Life expectancy", "type": "quantitative", "title": "Life Expectancy"},
        {"field": "Birth Rate", "type": "quantitative", "title": "Birth Rate"},
        {"field": "Population", "type": "quantitative", "title": "Population"},
        {"field": "GDP", "type": "quantitative", "title": "GDP"},
        {"field": "Unemployment rate", "type": "quantitative", "title": "Unemployment rate"},
        {"field": "Infant mortality", "type": "quantitative", "title": "Infant mortality"}
      ]
    }
}
